<!--family.wxml-->
<view class='container'>

  <view class='rank' wx:for='{{familyInfo}}' wx:key=''>
    <view class='family userInfo'>

      <image class='avatar' src='{{item.avatarurl}}'></image>

      <view class='userInfoMsg'>
        <view class='person'>
          <text class='name'>{{item.nickName}}</text>
          <image class='genderImg' src='{{genderImg[item.gender]}}'></image>
          <text class='age'>{{item.age}}</text>
        </view>
        <view class='area'>{{item.classname}}</view>
      </view>

      <view class='edit'>编辑</view>
      
    </view>
  </view>

  <view class='lastScroll'>
    <view class='hr'></view>
    <view class='text'>已经加载完毕啦</view>
    <view class='hr'></view>
  </view>

</view>
